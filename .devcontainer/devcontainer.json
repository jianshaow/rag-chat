{
	"name": "RAG-BE",
	"image": "jianshao/rag-chat-dev:0.0.2",
	"features": {
		"ghcr.io/devcontainers/features/git:1": {}
	},
	"remoteEnv": {
		"PYTHONPATH": "."
	},
	"mounts": [
		{"source": "${localEnv:HOME}/.ssh", "target": "/home/devel/.ssh", "type": "bind"}
	],
	"customizations": {
		"vscode": {
			"extensions": [
				"ms-python.python",
				"ms-python.vscode-pylance",
				"ms-python.black-formatter",
				"ms-python.isort",
				"alphabotsec.vscode-eclipse-keybindings"
			]
		}
	}
}
